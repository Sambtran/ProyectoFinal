                        </article>
                        <article>
                            <article id="OS">
                                <div>
                                    <canvas class="lienzoOS" id="lienzoOS1"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS2" class="lienzoOS"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS3" class="lienzoOS"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS4" class="lienzoOS"></canvas>
                                </div>
