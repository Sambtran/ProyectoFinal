</nav>
<main>
    <section>
        <div id="loginanonimo"><!--<img src="/images/imagen-mapamundi.jpeg">-->   <div>
                <div class="wave"></div>
                <div class="wave"></div>
                <div class="wave"></div>
            </div>></div>
    </section>
    <section>
        <div id="loginconuser"><!--<img src="/images/imagen-usuario.jpg">-->   <div>
                <div class="wave"></div>
                <div class="wave"></div>
                <div class="wave"></div>
            </div></div>
    </section>
    <section id="contenido">
        <div class="volver-atras-an"><p> SALIR</p></div>
        <div id="subcontenido">
        <article id="maincontenido">
            <section>
                <h2>Paises mas comunes</h2>
                <canvas id="lienzo1"></canvas>
            </section>
            <section>
                <h2>SO mas comunes</h2>
                <canvas id="lienzo2"></canvas>
            </section>
        </article></div>
    </section>
    <article id="logedg">
        <nav2 id="navusuario">
            <div>Comunes</div>
            <div>Custom query</div>
            <div>Historial</div>
        </nav2>
        <article id="auxiliar2">
            <article id="contgeneral">
                <aside id="tiposdegrafico">
                    <div class="tiposdegrafico">
                        Lineal
                    </div>
                    <div class="tiposdegrafico">
                        donut
                    </div>
                    <div class="tiposdegrafico">
                        3D
                    </div>
                </aside>
                <section id="loged">
                    <article id="lienzocontainers">
                        <aside class="page">
                            <div class="volver-atras"><p> SALIR</p></div>
                            <div class="container">
                                <div class="left">
                                    <div class="login">Login</div>
                                    <div class="eula"><p>Recordar en este navegador</p>
                                        <input id="recordar" type="checkbox"></div>
                                </div>
                                <div class="right">
                                    <svg viewBox="0 0 320 300">
                                        <defs>
                                            <linearGradient
                                                    inkscape:collect="always"
                                                    id="linearGradient"
                                                    x1="13"
                                                    y1="193.49992"
                                                    x2="307"
                                                    y2="193.49992"
                                                    gradientUnits="userSpaceOnUse">
                                                <stop
                                                        style="stop-color:#ff00ff;"
                                                        offset="0"
                                                        id="stop876"/>
                                                <stop
                                                        style="stop-color:#ff0000;"
                                                        offset="1"
                                                        id="stop878"/>
                                            </linearGradient>
                                        </defs>
                                        <path d="m 40,120.00016 239.99984,-3.2e-4 c 0,0 24.99263,0.79932 25.00016,35.00016 0.008,34.20084 -25.00016,35 -25.00016,35 h -239.99984 c 0,-0.0205 -25,4.01348 -25,38.5 0,34.48652 25,38.5 25,38.5 h 215 c 0,0 20,-0.99604 20,-25 0,-24.00396 -20,-25 -20,-25 h -190 c 0,0 -20,1.71033 -20,25 0,24.00396 20,25 20,25 h 168.57143"/>
                                    </svg>
                                    <div class="form">
                                        <label for="nickname">Nick</label>
                                        <input type="text" id="nickname">
                                        <label for="password">Password</label>
                                        <input type="password" id="password">
                                        <input type="submit" id="submit" value="Submit">
                                    </div>
                                </div>
                            </div>
                        </aside>
                        <article id="contenedordegraficos">
                            <article id="geo">
                                <div>
                                    <canvas class="lienzoGEO" id="lienzeogeo1"></canvas>
                                </div>
                                <div>

                                    <canvas class="lienzoGEO" id="lienzeogeo2"></canvas>
                                </div>
                                <div id="selectpaisdiv">
                                    <select id="selectpais">
                                        <% for (let X of todoslospaises){
                                        %>
                                            <% console.log("pais=  " + X.pais) %>
                                            <option value= <%= X.pais %>> <%= X.pais %> </option>
                                        <%
                                        } %>
                                    </select>
                                    <canvas class="lienzoGEO" id="lienzeogeo3"></canvas>
                                </div>
                                <div>
                                    <canvas class="lienzoGEO" id="lienzeogeo4"></canvas>
                                </div>
