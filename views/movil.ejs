        <nav2 id="navusuario">
            <div>Comunes</div>
            <div>Custom query</div>
            <div>Historial</div>
        </nav2>
        <article id="auxiliar2">
            <article id="contgeneral">
                <aside id="tiposdegrafico">
                    <div class="tiposdegrafico">
                        Barras
                    </div>
                    <div class="tiposdegrafico">
                        donut
                    </div>
                    <div class="tiposdegrafico">
                        3D
                    </div>
                </aside>
                <section id="loged">
                    <article id="lienzocontainers">
                        <div class="login-box page">

                            <h2>Login</h2>
                            <form>
                                <div class="user-box">
                                    <input type="text" id="nickname" name="" required="">
                                    <label>Username</label>
                                </div>
                                <div class="user-box" >
                                    <input type="password"  id="password" name="" required="">
                                    <label>Password</label>
                                </div>
                        <div id="cokiloged">
                            <p>Recordar en este navegador</p><input id="recordar" type="checkbox">
                        </div>
                            <a href="#" id="submit">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                    Submit
                                </a>
                            </form>
                        </div>
                        <article id="contenedordegraficos">
                            <article id="geo">
                                <div>
                                    <canvas class="lienzoGEO" id="lienzeogeo1"></canvas>
                                </div>
                                <div>

                                    <canvas class="lienzoGEO" id="lienzeogeo2"></canvas>
                                </div>
                                <div id="selectpaisdiv">
                                    <select id="selectpais">
                                        <% for (let X of todoslospaises){
                                        %>
                                            <% console.log("pais=  " + X.pais) %>
                                            <option value= <%= X.pais %>> <%= X.pais %> </option>
                                        <%
                                        } %>
                                    </select>
                                    <canvas class="lienzoGEO" id="lienzeogeo3"></canvas>
                                </div>
                                <div>
                                    <canvas class="lienzoGEO" id="lienzeogeo4"></canvas>
                                </div>
                            </article>
                        </article>
                        <article>
                            <article id="OS">
                                <div>
                                    <canvas class="lienzoOS" id="lienzoOS1"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS2" class="lienzoOS"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS3" class="lienzoOS"></canvas>
                                </div>
                                <div>
                                    <canvas id="lienzoOS4" class="lienzoOS"></canvas>
                                </div>
                            </article>
                        </article>
                    </article>
                </section>
            </article>
            <article id="customquery">
                <input id="sentencia" type="text">
                <button id="ejecutar">Ejectuar</button>
                <div id="mostradorp"></div>
            </article>
            <article id="historial">
