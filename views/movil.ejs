</head>
<body>
<nav>
    <h1>KAFKA ANALYZER</h1>
</nav>
<main>
    <section id="loginanonimo">
        Login Anonimo
    </section>
    <section id="loginconuser">
        Login con user
    </section>
    <section id="contenido">
        <div class="volver-atras-an"><p class="volver-atras-anp"> SALIR</p></div>
        <div id="subcontenido">
            <article id="maincontenido">
                <section>
                    <canvas id="lienzo1"></canvas>
                </section>
                <section>
                    <canvas id="lienzo2"></canvas>
                </section>
            </article></div>
